[
  {
    "selector": ":parent",
    "css": {
      "text-valign": "top",
      "text-halign": "center"
    }
  },
  {
    "selector": "edge",
    "css": {
      "curve-style": "bezier",
      "target-arrow-shape": "triangle"
    }
  },{
    "selector": "edge[arrow]",
    "css": {
      "curve-style": "bezier",
      "target-arrow-shape": "data(arrow)"
    }
  },
  {
